{
  "targets": {
    "debug": {
      "binaryFile": "build/debug.wasm",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "binaryFile": "build/release.wasm",
      "sourceMap": false,
      "optimize": true,
      "optimizeLevel": 3,
